{
  "version": 2,
  "name": "educhain-frontend",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir":  "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index. html"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://blockchain-backend-lnyq3.ondigitalocean.app/api"
  }
}
